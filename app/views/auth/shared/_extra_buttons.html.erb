<%= content_tag :div, class: "flex intems-center justify-center" do %>
  <%= link_to t('buttons.password_reset'), new_password_reset_path , class: "mx-2 btn-recover" if view.eql?(:login) && !current_user.present? %>
  <%= link_to t('buttons.register'), register_path, class: "mx-2 btn-signin" if view.eql?(:login) %>
<% end %>
